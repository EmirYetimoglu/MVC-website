@model IEnumerable<_20051350008.Models.VolunteerViewModel>


<thead>
    <tr>
        <th>Full Name</th>
        <th>Email</th>
        <th>Phone Number</th>
        <th>Address</th>
        <th>Date of Birth</th>
        <th>Gender</th>
        <th>Skills</th>
        <th>Availability</th>
        <th>Communication Method</th>
        <th>Profile Picture Path</th>
        <th>Options</th>
    </tr>
</thead>
<tbody>
    @foreach (var volunteer in Model)
    {
        <tr>
            <td>@volunteer.FullName</td>
            <td>@volunteer.Email</td>
            <td>@volunteer.PhoneNumber</td>
            <td>@volunteer.Address</td>
            <td>@volunteer.DateOfBirth</td>
            <td>@volunteer.Gender</td>
            <td>@string.Join(", ", volunteer.Skills)</td>
            <td>@volunteer.Availability</td>
            <td>@volunteer.CommunicationMethod</td>
            <td>@volunteer.ProfilePicturePath</td>
            <td>@Html.ActionLink("Edit", "Edit", new { id = volunteer.Id })</td>
        </tr>
    }
</tbody>



